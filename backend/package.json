{
  "name": "monolith",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ruuf:migration:migrate": "npm run build && npx typeorm migration:run -d dist/platform/database/typeorm-cli.config.js",
    "ruuf:migration:revert": "npx typeorm migration:revert -d dist/platform/database/typeorm-cli.config.js",
    "ruuf:migration:generate": "npm run build && npx typeorm migration:generate -d dist/platform/database/typeorm-cli.config.js",
    "db:setup": "npm run db:dev:create && npm run db:test:create",
    "db:dev:create": "docker compose up -d postgres-dev && npm run ruuf:migration:migrate",
    "db:dev:drop": "docker compose stop postgres-dev && docker compose rm -f postgres-dev && docker volume rm monolith_monolithdb",
    "db:test:create": "docker compose up -d postgres-test",
    "db:test:drop": "docker compose stop postgres-test && docker compose rm -f postgres-test",
    "db:seed": "ts-node -r tsconfig-paths/register src/platform/seeders/seed.app.ts"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.218.0",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "handlebars": "^4.7.7",
    "mercadopago": "^1.5.14",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/jest-when": "^3.5.2",
    "@types/mercadopago": "^1.5.8",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-when": "^3.5.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../test/jest-setup-file.ts"
    ]
  }
}
